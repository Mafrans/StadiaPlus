{
    "name": "Stadia+ Extension [Beta]",
    "short_name": "Stadia+",
    "version": "3.0.0.2",
    "author": "Malte Kl√ºft (Mafrans)",
    "description": "Extends Google's Stadia gaming platform with various network features, such as statistics and achievement-tracking.",
    "key": "",
    "permissions": [
        "declarativeContent",
        "storage",
        "identity",
        "https://stadiagamedb.com/data/gamedb.json",
        "https://stadiagamedb.com/data/uuidmap.json",
        "https://*.stadiaplus.dev/*",
        "https://*.chromiumapp.org/*"
    ],
    "oauth2": {
        "client_id": "401608975485-hcg4ipsil0inrtvbmgjl2e910nceqlh0.apps.googleusercontent.com",
        "scopes": [
            "https://beta.stadiaplus.dev/auth/google",
            "https://beta.stadiaplus.dev/auth/google/callback"
        ]
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "dist/popup/index.html",
        "default_icon": {
            "16": "images/Stadia+16.png",
            "32": "images/Stadia+32.png",
            "48": "images/Stadia+48.png",
            "128": "images/Stadia+128.png"
        }
    },
    "icons": {
        "16": "images/Stadia+16.png",
        "32": "images/Stadia+32.png",
        "48": "images/Stadia+48.png",
        "128": "images/Stadia+128.png"
    },
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "https://stadia.google.com/*"
            ],
            "js": [
                "dist/main/index.js"
            ],
            "css": [
                "dist/main/index.css"
            ]
        }
    ],
    "web_accessible_resources": [
        "dist/main/*.png",
        "dist/popup/*.png"
    ],
    "manifest_version": 2
}